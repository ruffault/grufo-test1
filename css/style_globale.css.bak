/** 
 * Carrousel style.
 */
#slideshow {
	text-align: center; 

	margin:auto;
	width:580px;
	height:210px;
	position:relative;

    -padding-left:-10px; 
}
#slideshow #slidesContainer {

  margin:0 auto;
  width:495px;
  height:210px;
  overflow:auto; /* allow scrollbar */
  position:relative; 	
}
#slidesContainer {
				background:transparent url(../images/carousel_titre.jpg) repeat-x 0 0;

background-color: #f8f8f6;


}
#slideshow #slidesContainer .slide {
		text-align: left; 

  margin:0 auto;
  width:440px; /* reduce by 20 pixels of #slidesContainer to avoid horizontal scroll */
  height:210px;
}

/** 
 * Carrousel controle.
 */
.control {
  display:block;
  width:39px;
  height:210px;
  text-indent:-10000px;
  position:absolute;
  
  cursor: pointer;
}
#leftControl {
  top:0;
  left:2px;
  background:transparent url(control_left2.jpg) no-repeat 0 0;
}
#rightControl {
  top:0;
  right:2px;
  background:transparent url(control_right2.jpg) no-repeat 0 0;
}

