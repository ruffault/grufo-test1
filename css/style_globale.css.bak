}

.left {
	width: 200px;
	margin-left: -200px;
	float: left;
	position: relative;
	z-index: 10;
	margin-right: 1px;
	text-align: left; 

}

.right {
	width: 200px;
	margin-right: -200px;
	float: left;
	position: relative;
	z-index: 11;
	margin-left: 1px;
	text-align: left; 
}
.header{text-align: left;
	background-color: #fff;
}

/* Barre de navigation*/
#nav {
	float: left;
	width: 100%;
	background: #ffffff url("bg.gif") repeat-x bottom;
	font-size: 93%;
	line-height: normal;
}

#nav ul {
	margin: 0;
	padding: 10px 10px 0;
	list-style: none;
}

#nav li {
	float: left;
	background: url("left.gif") no-repeat left top;
	margin: 0;
	padding: 0 0 0 9px;
}

#nav a{
	float: left;
	display: block;
	background: url("right.gif") no-repeat right top;
	padding: 5px 15px 4px 6px;
	text-decoration: none;
	font-weight: bold;
	color: #765;
	
}

#nav #current { background-image:url("left_on.gif"); }

#nav #current a {
	background-image: url("right_on.gif");
	padding-bottom: 5px;
	color: #432;
}

#mininavi {
	float:right;
	text-align:right;
}

#mininavi img{
  vertical-align:middle;
  margin-right:3px;
}

/* titre, logo et navigation */
.header h1, .header ul, .header p{ display: none; }

#logo {
	background: url("logo.gif") no-repeat left bottom;
	display: block;
	height: 67px;
	width: 343px;

}

/* Barre de recherche */
#search
{
	background: #ffa500;
	text-align: center;
	border-bottom: 1px solid #666;
	font-weight: bold;
	clear: both;
	padding: 5px;
	height: 2.2em;
}

#search input{ vertical-align:middle; }

#search form, #search p {
  margin: 0;
  padding: 0;
}

/* Main code */
.outer {
	width: auto;
	border-left-style: solid;
	border-right-style: solid;
}

.inner {
	margin: 0;
	width: 100%;

}

.center {
	float: left;
	width: 100%;
	position: relative; 
	z-index: 12;
}

.wide {
	width: 100%;
	position: relative;
	z-index: 13;
}

.clear { clear: both; }

hr {
	clear: both;
	visibility: hidden;
	margin: 0;
}

/* Mozilla code */
.outer > .inner { border-bottom: 1px solid white; }
